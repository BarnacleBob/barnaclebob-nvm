{
  "name": "barnaclebob-nvm",
  "version": "1.0.2",
  "author": "barnaclebob",
  "summary": "Module to manage multiple nodejs versions per user using nvm",
  "license": "Apache-2.0",
  "source": "https://github.com/BarnacleBob/barnaclebob-nvm",
  "project_page": "https://github.com/BarnacleBob/barnaclebob-nvm",
  "issues_url": "https://github.com/BarnacleBob/barnaclebob-nvm/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.2.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "10.04"
      ]
    }
  ]
}
